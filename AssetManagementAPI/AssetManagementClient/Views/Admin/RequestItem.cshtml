@{
    ViewData["Title"] = "Dashboard";
    Layout = "~/Views/Shared/_LayoutDashboardAdmin.cshtml";
}
<div class="page-wrapper">
    <div class="p-3">
        <div class="page-breadcrumb">
            <div class="row">
                <div class="col-7 align-self-center">
                    <h3 class="page-title text-truncate text-dark font-weight-medium mb-1">Request Asset Management</h3>
                    <div class="d-flex align-items-center">
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb m-0 p-0">
                                <li class="breadcrumb-item">
                                    <a href="#">Menu</a>
                                </li>
                                <li class="breadcrumb-item active" aria-current="page">
                                    <a href="#">Request Item</a>
                                </li>
                                <li class="breadcrumb-item active" aria-current="page">
                                    <a href="#">History</a>
                                </li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
        <div class="container-fluid mt-5">
            <p></p>
            <table id="tabledatauserrequesthistory" class="table table-striped table-bordered dataTable" style="width:100%">
                <thead class="thead-light">
                    <tr>
                        <th>No</th>
                        <th>Id</th>
                        <th>Item</th>
                        <th>Name</th>
                        <th>Start Date</th>
                        <th>End date</th>
                        <th>Notes</th>
                        <th>Quantity</th>
                        <th>Status</th>
                    </tr>
                </thead>
                <tfoot>
                    <tr>
                        <th>No</th>
                        <th>Id</th>
                        <th>Item</th>
                        <th>Name</th>
                        <th>Start Date</th>
                        <th>End date</th>
                        <th>Notes</th>
                        <th>Quantity</th>
                        <th>Status</th>
                    </tr>
                </tfoot>
                <tbody></tbody>
            </table>
        </div>
        <div class="container-fluid mt-5">
    </div>
</div>
</div>